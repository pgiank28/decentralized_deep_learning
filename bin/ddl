#!/bin/bash

if [ -z "$SPARK_HOME" ]; then
    echo "spark home not found"
fi

/usr/local/spark/bin/spark-submit --class ddlspark.examples.simple --packages ddlspark:core:1.0-SNAPSHOT  /home/pantelisg/DDLSpark/decentralized_deep_learning/examples/basic_examples3/target/examples.basic_examples3-1.0-SNAPSHOT.jar 
